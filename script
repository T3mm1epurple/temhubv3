    if mainid == 4472538305 then
        local Working = Window:CreateTab("Working", 13813196847)
        local Lottery = Window:CreateTab("Tickets", 13813097941)
        local Other = Window:CreateTab("Other", 13813123052)

        local Section = Working:CreateSection("Seating")

        local Button = Working:CreateButton({
            Name = "Find seat",
            Callback = function()
                for i, computer in pairs(workspace.COMPUTERS:GetChildren()) do
                    if game.Players.LocalPlayer.Character.Humanoid.Sit == true then
                        break
                    end
                    if not computer.Seat.Seat:FindFirstChild("SeatWeld") then
                        computer.Seat.Seat:Sit(game.Players.LocalPlayer.Character.Humanoid)
                        break
                end
            end    
            end,
        })

        local Section = Lottery:CreateSection("Extras")

        local Button = Lottery:CreateButton({
            Name = "Get lottery ticket",
            Callback = function()
                local num = math.random(1, 99)
                if #tostring(num) == 1 then
                    num = "0"..tostring(num)
                end
            local args = {
                [1] = 768,
                [2] = tostring(num)
            }
            
            game:GetService("ReplicatedStorage").BoomEvent:FireServer(unpack(args))    
            end,
        })

        local Button = Lottery:CreateButton({
            Name = "Throw away lottery ticket",
            Callback = function()
                game:GetService("ReplicatedStorage").ThrowEvent:FireServer()
            end,
        })

        local Section = Lottery:CreateSection("Main")

        local suitchangetoggle = Lottery:CreateToggle({
            Name = "Auto Lottery ticket",
            CurrentValue = nil,
            Flag = nil,
            Callback = function(callback)
                local value = "autolottery"
                    originaldata[value] = callback
                    coroutine.resume(coroutine.create(function()
                        while originaldata[value] == true do
                            local yes, no = pcall(function()
                            if scriptrunning then
                                if owneringame == false then
                                if not plr.Backpack:FindFirstChild("Card") and not plr.Character:FindFirstChild("Card") then
                                    local num = math.random(1, 99)
                                    if #tostring(num) == 1 then
                                        num = "0"..tostring(num)
                                    end
                                local args = {
                                    [1] = 768,
                                    [2] = tostring(num)
                                }
                                
                                game:GetService("ReplicatedStorage").BoomEvent:FireServer(unpack(args)) 
                            end
                        end
                            end
                        end)
                        wait()
                        end
                    end))
            end,
         })
         suitchangetoggle:Set(originaldata["autolottery"])

         local ticketnumber = Lottery:CreateLabel("Ticket number: ")
         local currentnumber = Lottery:CreateLabel("Chosen number: ")
         coroutine.resume(coroutine.create(function()
            while wait() do
                if scriptrunning then
                    currentnumber:Set("Chosen number: "..workspace.NUMBERCHOOSER.Model.Model.Number.SurfaceGui.No.Text)
                    local yes, no = pcall(function()
                    ticketnumber:Set("Ticket number: "..plr.Character:FindFirstChild("Card").ToolTip or plr.Backpack:FindFirstChild("Card").ToolTip)
                    end)
                end
            end
         end))

         local Section = Lottery:CreateSection("Fun Stuff")
         local sillycardamount = 1
         local getcard = true

         local cardamounttoggle = Lottery:CreateSlider({
            Name = "Card amount",
            Range = {1, 100},
            Increment = 1,
            Suffix = "Number",
            CurrentValue = sillycardamount,
            Flag = nil,
            Callback = function(Value)
             sillycardamount = Value
            end,
         })
         cardamounttoggle:Set(sillycardamount)

         local Button = Lottery:CreateButton({
            Name = "Get card amount",
            Callback = function()
                for number=1, sillycardamount do
                    if getcard then
                    for i, tool in pairs(plr.Backpack:GetChildren()) do
                        if tool.Name == "Card" then
                        tool.Parent = plr.Character
                    end
                    end
                    local toolamount = #plr.Backpack:GetChildren()
                    local args = {
                        [1] = 768,
                        [2] = tostring(number)
                    }
                    
                    game:GetService("ReplicatedStorage").BoomEvent:FireServer(unpack(args))
                    repeat wait()
                    until #plr.Backpack:GetChildren() > toolamount
                    else
                        getcard = true
                        break
                end
                end
            end,
        })

        local Button = Lottery:CreateButton({
            Name = "Stop getting card count",
            Callback = function()
                getcard = false
            end,
        })

        local Button = Other:CreateButton({
            Name = "Get obby hat",
            Callback = function()
                local hat = workspace.ObbyHat
                local originalcframe = plr.Character.HumanoidRootPart.CFrame
                plr.Character.HumanoidRootPart.CFrame = hat.CFrame
                repeat wait()
                    fireclickdetector(hat.ClickDetector)
                until plr.attributes.Level3Obby.Value == 1
                plr.Character.HumanoidRootPart.CFrame = originalcframe
            end,
        })
    end

