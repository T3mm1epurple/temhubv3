                    end
                end,
            })

            local ewrfoij = false

            local extracttoggle = Sample:CreateToggle({
                Name = "Auto Extract",
                CurrentValue = nil,
                Flag = nil,
                Callback = function(callback)
                    local value = "extract"
                        originaldata[value] = callback
                        coroutine.resume(coroutine.create(function()
                            while originaldata[value] == true do
                                local yes, no = pcall(function()
                                if scriptrunning then
                                    if owneringame == false then
                                    if plr.attributes.Unknown.Value == "" then
                                for i, obj in pairs(workspace.PossibleSpawns:GetChildren()) do
                                    if obj:FindFirstChild("SampleNPC") then
                                        local gofor = true
                                        local color = obj.SampleNPC.CanPoke:FindFirstChildOfClass("BoolValue").Name
                                        if originaldata["Sample"..color] == false then
                                            gofor = false
                                        end
                                        if gofor then
                                        local npc = obj.SampleNPC
                                        local root = npc.HumanoidRootPart
                                        local tool = nil
                                        if plr.Backpack:FindFirstChild("Extractor") then
                                            tool = plr.Backpack.Extractor
                                            tool.Parent = plr.Character
                                        else
                                            tool = plr.Character:FindFirstChild("Extractor")
                                        end
                                        if tool ~= nil then
                                            if tool.Parent == plr.Character then
                                                local originalcframe = root.CFrame
                                                repeat wait()
                                                    root.CFrame = tool.Union.CFrame
                                                    tool:Activate()
                                                until plr.attributes.Unknown.Value ~= ""
                                                root.CFrame = originalcframe
                                            end
                                        end
                                    end
                                end
                                end
                            else
                                for i, lab in pairs(workspace.Labs:GetChildren()) do
                                    if lab.Owner.Value == plr then
                                        local count = 0
                                        for i=1, 10 do
                                            local val = plr.attributes:FindFirstChild("VialStorage"..i)
                                            if val then
                                                if val.Value ~= "" then
                                                    count += 1
                                                end
                                            end
                                        end
                                        if count < 10 then
                                            plr.Character.HumanoidRootPart.CFrame = CFrame.new(lab.Checker.Base.Position) + Vector3.new(0, 3, 0)
                                        repeat wait()
                                            fireproximityprompt(lab.Checker.Base.Attachment.ProximityPrompt)
                                        until plr.attributes.Unknown.Value == ""
                                        local num = 0
                                        repeat wait(0.5)
                                            num += 0.5
                                            fireproximityprompt(lab.Checker.Base.Attachment.ProximityPrompt)
                                            until lab.Checker.Time.TextLabel.Text == "" or num == 3
                                            if plr.Backpack:FindFirstChild("Failed") then
                                                plr.Backpack.Failed.Parent = plr.Character
                                                plr.Character.CFrame = CFrame.new(lab.Dispose.Main.Position) + Vector3.new(0, 5, 0)
                                                repeat wait()
                                                    fireproximityprompt(lab.Dispose.Main.Attachment.ProximityPrompt)
                                                    until not plr.Character:FindFirstChild("Failed")
                                            end
                                        end
                                        if count >= 10 then
                                            if ewrfoij == false then
                                                ewrfoij = true
                                            savedata()
                                            TeleportService:Teleport(4684427971)
                                                queueteleport([[
                                                    wait(3)
                                                        local args = {
                                                            [1] = 3,
                                                            [2] = {
                                                                [1] = true,
                                                                [2] = true,
                                                                [3] = true,
                                                                [4] = true,
                                                                [5] = true,
                                                                [6] = true,
                                                                [7] = true,
                                                                [8] = true,
                                                                [9] = true,
                                                                [10] = true
                                                            }
                                                        }
                                                        
                                                        game:GetService("ReplicatedStorage"):WaitForChild("DoctorEvent"):FireServer(unpack(args))  
                                                        fireclickdetector(workspace.VialForSale.ClickDetector)
                                                        TeleportService = game:GetService("TeleportService")
                                                        TeleportService:Teleport(8637121738)
                                                        local queueteleport = syn and syn.queue_on_teleport or queue_on_teleport or fluxus and fluxus.queue_on_teleport or function() end   
                                                        queueteleport([=[
                                                            repeat wait() until game.Players.LocalPlayer.Character ~= nil
                                                            _G.erg5437654634t3gbergjwoi4tOITGJERIOGHERg43 = true
                                                            loadstring(game:HttpGet("https://raw.githubusercontent.com/kittenis/temhubv3/main/script", true))()
                                                            for i, obj in pairs(workspace.Labs:GetChildren()) do
                                                                if obj.Owner.Value == nil then
                                                                    local plr = game.Players.LocalPlayer
                                                                    plr.Character.HumanoidRootPart.CFrame = CFrame.new(obj.Door.RealDoor.Glass.Position)
                                                                    repeat wait() 
                                                                    fireproximityprompt(obj.Door.RealDoor.Glass.ProximityPrompt)
                                                                    until obj.Owner.Value ~= nil
                                                                    break
                                                                end
                                                            end
                                                            ]=])                                              
                                                                    ]])
                                end
                            end
                            end
                        end
                                end
                            end
                        end
                        end)
                            wait()
                        end
                        end))
                end,
             })
