local gameids = {
    ["level1"] = 4050215579,
    ["level3"] = 4472538305,
    ["level4"] = 4684427971,
    ["level5"] = 5248557397,
    }
    
    local placeid = game.PlaceId
    local plr = game.Players.LocalPlayer
    local queueteleport = syn and syn.queue_on_teleport or queue_on_teleport or fluxus and fluxus.queue_on_teleport or function() end

    local originaldata = {  
        autobingo = false,
        mysteryhat = false,
