                                                                    else
                                                                        break
                                                                    end
                                                                until plr.PlayerGui:FindFirstChild("GuiMessage")
                                                                if plr.PlayerGui:FindFirstChild("GuiMessage") then
                                                                plr.PlayerGui:FindFirstChild("GuiMessage"):Destroy()
                                                                end
                                                            end
                                                        end
                                                    else
                                                        if ermmwhat == false then
                                                            ermmwhat = true
