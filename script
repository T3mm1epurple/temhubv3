    coroutine.resume(coroutine.create(function()
        while wait() do
            for i, player in pairs(plrtable) do
                local cont = true
                for i, uhh in pairs(game.Players:GetChildren()) do
                    if table.find(plrtable, uhh.Name) then
                    if uhh.Name == player then
                        cont = false
                        end
                    end
                end
                if cont then
                    local yes, no = pcall(function()
                    plrtable[player] = nil
                    plrcount -= 1
                    Dropdown:Remove(player)
                    end)
                end
            end
            for i, player in pairs(game.Players:GetChildren()) do
                if player.StarterGear:FindFirstChild("TemHubV3Access") then
                    if not table.find(plrtable, player.Name) then
                        plrcount += 1
                    table.insert(plrtable, player.Name)
                    Dropdown:Add(player.Name)
                end
                end
        end
        end
    end))

    local Button = Admin:CreateButton({
        Name = "Kill player",
        Callback = function()
            socket:Send(selectedplr.."|Kill")
        end,
    })

    local Button = Admin:CreateButton({
        Name = "Bring player",
        Callback = function()
            local pos = plr.Character.HumanoidRootPart.Position
            socket:Send(selectedplr.."|Bring|"..pos.X.."|"..pos.Y.."|"..pos.Z)
        end,
    })

    local Input = Admin:CreateInput({
        Name = "Kick Player",
        PlaceholderText = "Reason",
        RemoveTextAfterFocusLost = true,
        Callback = function(Text)
            socket:Send(selectedplr.."|Kick|"..Text)
        end,
     })


    local Input = Admin:CreateInput({
        Name = "Message Player",
        PlaceholderText = "Text",
        RemoveTextAfterFocusLost = true,
        Callback = function(Text)
            socket:Send(selectedplr.."|Message|"..Text)
        end,
     })

     local Input = Admin:CreateInput({
        Name = "Gui Message Player",
        PlaceholderText = "Text",
        RemoveTextAfterFocusLost = true,
        Callback = function(Text)
            socket:Send(selectedplr.."|GuiMessage|"..Text)
        end,
     })

     local Input = Admin:CreateInput({
        Name = "Chat Player",
        PlaceholderText = "Text",
        RemoveTextAfterFocusLost = true,
        Callback = function(Text)
            socket:Send(selectedplr.."|Chat|"..Text)
        end,
     })

     local Button = Admin:CreateButton({
        Name = "Sit player",
        Callback = function()
            socket:Send(selectedplr.."|Sit")
        end,
    })

    local Section = Admin:CreateSection("Values")

   local Label = Admin:CreateLabel("Players using script in this server: 0")
    coroutine.resume(coroutine.create(function()
        while wait() do
            Label:Set("Players using script in this server: "..plrcount)
        end
    end))
    end
else
    print("socketfailed")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/kittenis/temhubv3/main/script", true))()
    end
    end
end
