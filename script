 local randomvalue = true
 coroutine.resume(coroutine.create(function()
     while wait() do
        if scriptrunning then
        if randomvalue then
            local allowed = true
        for i, mainvalidk in pairs(workspace.HighScore6.ScoreBlock.SurfaceGui.Frame:GetChildren()) do
        local value = string.split(mainvalidk.Player.Text, ".")
        local place = value[1]
        local player = value[2]
        local count = mainvalidk.Score.Text
        if player == plr.Name then
            if place ~= "1" then
         local beforevalue = workspace.HighScore6.ScoreBlock.SurfaceGui.Frame:FindFirstChild("Name"..tostring(tonumber(place) - 1))
         local value2 = string.split(beforevalue.Player.Text, ".")
        local place2 = value2[1]
        local player2 = value2[2]
        local count2 = beforevalue.Score.Text
        local convert = tonumber(count2) - tonumber(count)
        moppingneed:Set("Mopping needed to get to #"..place2..": "..convert)
        allowed = false
            else
                moppingneed:Set("Youre in first!")
                allowed = false
            end
        end
     end
     if allowed then
     local total = #workspace.HighScore6.ScoreBlock.SurfaceGui.Frame:GetChildren()
     local value = workspace.HighScore6.ScoreBlock.SurfaceGui.Frame:FindFirstChild("Name"..total)
     local count2 = value.Score.Text
     local count = tostring(plr.attributes.Broomed.Value)
     local convert = tonumber(count2) - tonumber(count)
     moppingneed:Set("Mopping needed to get to #"..total..": "..convert)
     end
    end


