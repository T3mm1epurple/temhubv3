            local determinevalue = Instance.new("BoolValue")
            determinevalue.Name = "TemHubV3Access"
            determinevalue.Parent = plr.StarterGear
        end
        if msg == "KillAll" then
            plr.Character.Humanoid.Health = 0
        end
        local data = msg:split("|")
        if data[2] == "OtherAdd" then
            for i, extraplr in pairs(game.Players:GetChildren()) do
                if extraplr.Name == data[1] then
                    if not extraplr.StarterGear:FindFirstChild("TemHubV3Access") then
