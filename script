local goto4 = false
local atthispoint = Drinking:CreateToggle({
    Name = "Auto buy selected drink",
    CurrentValue = nil,
    Flag = nil,
    Callback = function(callback)
        local value = "autosoda"
            originaldata[value] = callback
            coroutine.resume(coroutine.create(function()
                while originaldata[value] do
                    local yes, no = pcall(function()
                    if scriptrunning then
                        if owneringame == false then
                    if plr.attributes.Sick.Value > 0 then
                        goto4 = true
                        originaldata[value] = false
                        Rayfield:Notify({
                            Title = "Sick!",
                            Content = "you arent able to buy the drink because youre sick, go to level 4?",
                            Duration = 1,
                            Image = 4483362458,
                            Actions = { -- Notification Buttons
                            Ignore = {
                                Name = "No",
                                Callback = function()
                                  print("user said no")
                             end
                          },
                               Yes = {
                                  Name = "Yes",
                                  Callback = function()
                                    savedata()
                                    floorteleport(4)
                                    queueteleport([[
                                    wait(3)
                                    loadstring(game:HttpGet("https://raw.githubusercontent.com/kittenis/temhubv3/main/script", true))()
                                    ]])
                                    Rayfield:Notify({
                                        Title = "Notification",
                                        Content = "Teleporting to floor 4..",
                                        Duration = 1,
                                        Image = 4483362458,
                                        Actions = { -- Notification Buttons
                                           --[[Ignore = {
                                              Name = "Okay!",
                                              Callback = function()
                                              print("The user tapped Okay!")
                                           end
                                        },]]--
                                     },
                                     })
                               end
                            },
                         },
                         })
                        return
                    end
                    if plr.Character ~= nil and plr.Character:FindFirstChild("Humanoid") then
                        if plr.Character.Humanoid.Health > 0 then
                if not plr.Backpack:FindFirstChild(originaldata["soda"]) and not plr.Character:FindFirstChild(originaldata["soda"]) then
                    local pos = Vector3.new(0, 0, 0)
        if originaldata["sodamode"] == 3 then
        pos = workspace.RoofP.Position + Vector3.new(0, 10, 0)
        elseif originaldata["sodamode"] == 2 then
        pos = plr.Character.HumanoidRootPart.Position
        end
        for i,object in pairs(workspace["711Stuff"]:GetChildren()) do
            if object.Name == originaldata["soda"] then
                if plr.Character ~= nil and plr.Character:FindFirstChild("Humanoid") then
                    if plr.Character.Humanoid.Health > 0 then
                repeat wait(1)
                until not plr.Character.Head:FindFirstChildOfClass("ParticleEmitter") or plr.Character.Humanoid.Health <= 0
                plr.Character.HumanoidRootPart.CFrame = object.Source.CFrame
                fireproximityprompt(object.Source.Attachment.ProximityPrompt)
                repeat wait()
                    if plr.Character ~= nil and plr.Character:FindFirstChild("Humanoid") then
                        if plr.Character.Humanoid.Health > 0 then
                    fireproximityprompt(object.Source.Attachment.ProximityPrompt)
                    end
                end
                until plr.Backpack:FindFirstChild(originaldata["soda"]) or plr.Character.Humanoid.Health <= 0 or plr.Character:FindFirstChild(originaldata["soda"]) or originaldata[value] == false
                if plr.Character ~= nil and plr.Character:FindFirstChild("Humanoid") then
                    if plr.Character.Humanoid.Health > 0 then
                    if originaldata["sodamode"] > 1 then
                        if owneringame == false then
                plr.Character.HumanoidRootPart.CFrame = CFrame.new(pos)
                        end
                    end
                end
                end
            end
            end
            end
        end
                end
            end
        end
    end
    end
end)
                wait()
                end
            end))
    end,
 })
 atthispoint:Set(originaldata["autosoda"])
 coroutine.resume(coroutine.create(function()
    while wait() do
        if scriptrunning then
        if goto4 == true then
            goto4 = false
        atthispoint:Set(false)
        end
    else
        break
        end
    end
end))

