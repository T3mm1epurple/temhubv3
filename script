 local dog = false
 local moptoggle = Cleaning:CreateToggle({
    Name = "Auto mop",
    CurrentValue = nil,
    Flag = nil,
    Callback = function(callback)
        local value = "automop"
            originaldata[value] = callback
            coroutine.resume(coroutine.create(function()
                while originaldata[value] == true do
                    local moptppoint = workspace.RoofP.Position + Vector3.new(0, 10, 0)
                for i,vomit in pairs(workspace:GetChildren()) do
                    if originaldata[value] == true then
                    if vomit.Name == "VomitPart" then
                        if vomit.Parent ~= nil then
                        plr.Character.HumanoidRootPart.CFrame = vomit.CFrame + Vector3.new(0, 5, 0)
                        repeat wait()
                            if vomit.Parent ~= nil then
                            fireproximityprompt(vomit:FindFirstChildOfClass("ProximityPrompt"))
                            end
                        until plr.Character:FindFirstChild("MOPHATArm") or plr.Character:FindFirstChild("BROOOMHATArm") or vomit.Parent == nil
                        plr.Character.HumanoidRootPart.CFrame = CFrame.new(moptppoint)
                        repeat wait()
                        until not plr.Character:FindFirstChild("MOPHATArm") and not plr.Character:FindFirstChild("BROOOMHATArm")
                    end
                end
            else
                break
            end
                end
                if originaldata["getcleansolution"] == true then
                    if plr.attributes.CleanWater.Value <= 0 then
                        if dog == false then
                            dog = true
                        savedata()
                        floorteleport(4)
                                    queueteleport([[
                                        wait(3) 
                                        for i, model in pairs(workspace:GetChildren()) do
                                        if model.Name == "Cleaning Solution" then
                                            fireclickdetector(model:FindFirstChildOfClass("ClickDetector"))
                                            local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/T3mm1epurple/rayfieldedit/main/script'))()
        
                            local Window = Rayfield:CreateWindow({
                               Name = "",
                               LoadingTitle = "Got cleaning solution, teleporting back to level 1/2",
                               LoadingSubtitle = "just for notifications",
                            })
                                            wait(5)
                                            local args = {
                                                [1] = 1
                                            }
                                            
                                            game:GetService("ReplicatedStorage").TeleportEvent:FireServer(unpack(args))
                                                queueteleport([=[
                                                    wait(5) 
                                                    game.Players.LocalPlayer.PlayerGui.Menu.Enabled = false 
                                                    workspace.CurrentCamera.CameraType = Enum.CameraType.Custom workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character.Humanoid 
                                                    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
                                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.CleanBucket.Bucket.CFrame 
                                                    wait(1) 
                                                    fireproximityprompt(workspace.CleanBucket.Bucket.Attachment.ProximityPrompt)
                                                    wait(3)
                                                    loadstring(game:HttpGet("https://raw.githubusercontent.com/kittenis/temhubv3/main/script", true))()
                                                ]=])
                                        end
                                    end]])
                    end
                    end
                end
                wait()
                end
            end))
    end,
 })
 moptoggle:Set(originaldata["automop"])

 local Section = Cleaning:CreateSection("Extras")

 local autosoluttoggle = Cleaning:CreateToggle({
    Name = "Auto get cleaning solution",
    CurrentValue = nil,
    Flag = nil,
    Callback = function(callback)
        local value = "getcleansolution"
            originaldata[value] = callback
    end,
 })
 autosoluttoggle:Set(originaldata["getcleansolution"])

 local Section = Delivery:CreateSection("Coming soon (making better than before)")

 local Section = FunTab:CreateSection("Suits")

 local suitspeedslider = FunTab:CreateSlider({
    Name = "Suit change speed",
    Range = {1, 100},
    Increment = 1,
    Suffix = "Number",
    CurrentValue = 5,
    Flag = nil,
    Callback = function(Value)
     local value = "suitspeed"
    originaldata[value] = Value
    end,
 })
 suitspeedslider:Set(originaldata["suitspeed"])

 local suitchangetoggle = FunTab:CreateToggle({
    Name = "Auto suit change",
    CurrentValue = nil,
    Flag = nil,
    Callback = function(callback)
        local value = "suitchange"
            originaldata[value] = callback
            coroutine.resume(coroutine.create(function()
                while originaldata[value] == true do
                    for i, part in pairs(workspace:GetChildren()) do
                        if part:FindFirstChildOfClass("Decal") and part:FindFirstChild("Sound") and part:FindFirstChild("ClickDetector") and part:FindFirstChild("Price") then
                            if originaldata[value] == true then
                                fireclickdetector(part["ClickDetector"])
                                wait(5/originaldata["suitspeed"])
                            else
                                break
                            end
                        end
                    end
                wait()
                end
            end))
    end,
 })
 suitchangetoggle:Set(originaldata["suitchange"])

 local Section = FunTab:CreateSection("Other")

 local Input = FunTab:CreateInput({
    Name = "Message",
    PlaceholderText = "Text",
    RemoveTextAfterFocusLost = true,
    Callback = function(Text)
        local message = game.ReplicatedStorage.Message:Clone()
        message.Title.Text = Text
        message.Parent = plr.PlayerGui
    end,
 })

 local Input = FunTab:CreateInput({
    Name = "Gui Message",
    PlaceholderText = "Text",
    RemoveTextAfterFocusLost = true,
    Callback = function(Text)
        local message = game.ReplicatedStorage["GuiMessage"]:Clone()
        message.Frame.TextLabel.Text = Text
        message.Parent = plr.PlayerGui
    end,
 })
    end

    if mainid == gameids["level3"] then
        local Lottery = Window:CreateTab("Lottery", 4483362458)

        local Section = Lottery:CreateSection("Extras")

        local Button = Lottery:CreateButton({
            Name = "Get lottery ticket",
            Callback = function()
                local args = {
                    [1] = 768,
                    [2] = tostring(math.random(10, 99))
                }
                
                game:GetService("ReplicatedStorage").BoomEvent:FireServer(unpack(args))   
            end,
        })

        local Button = Lottery:CreateButton({
            Name = "Throw away lottery ticket",
            Callback = function()
                game:GetService("ReplicatedStorage").ThrowEvent:FireServer()
            end,
        })

        local Section = Lottery:CreateSection("Main")

        local suitchangetoggle = Lottery:CreateToggle({
            Name = "Auto Lottery ticket",
            CurrentValue = nil,
            Flag = nil,
            Callback = function(callback)
                local value = "autolottery"
                    originaldata[value] = callback
                    coroutine.resume(coroutine.create(function()
                        while originaldata[value] == true do
                                local args = {
                                    [1] = 768,
                                    [2] = tostring(math.random(10, 99))
                                }
                                
                                game:GetService("ReplicatedStorage").BoomEvent:FireServer(unpack(args))   
                        wait()
                        end
                    end))
            end,
         })
         suitchangetoggle:Set(originaldata["autolottery"])

         local Section = Lottery:CreateSection("Fun Stuff")
         local sillycardamount = 1
         local getcard = true

         local cardamounttoggle = Lottery:CreateSlider({
            Name = "Card amount",
            Range = {1, 100},
            Increment = 1,
            Suffix = "Number",
            CurrentValue = sillycardamount,
            Flag = nil,
            Callback = function(Value)
             sillycardamount = Value
            end,
         })
         cardamounttoggle:Set(sillycardamount)

         local Button = Lottery:CreateButton({
            Name = "Get card amount",
            Callback = function()
                for number=1, sillycardamount do
                    if getcard then
                    for i, tool in pairs(plr.Backpack:GetChildren()) do
                        if tool.Name == "Card" then
                        tool.Parent = plr.Character
                    end
                    end
                    local toolamount = #plr.Backpack:GetChildren()
                    local args = {
                        [1] = 768,
                        [2] = tostring(number)
                    }
                    
                    game:GetService("ReplicatedStorage").BoomEvent:FireServer(unpack(args))
                    repeat wait()
                    until #plr.Backpack:GetChildren() > toolamount
                    else
                        getcard = true
                        break
                end
                end
            end,
        })

        local Button = Lottery:CreateButton({
            Name = "Stop getting card count",
            Callback = function()
                getcard = false
            end,
        })
    end

    if mainid == gameids["level4"] then
        local Doctor = Window:CreateTab("Doctor", 4483362458)

        local Button = Doctor:CreateButton({
            Name = "Fast Checkup",
            Callback = function()
                plr.Character.HumanoidRootPart.CFrame = workspace["nurse waman"].HumanoidRootPart.CFrame
                        repeat wait()
                        fireclickdetector(workspace["nurse waman"].ClickDetector)
            until plr.PlayerGui:FindFirstChild("Nurse")
                        for i, message in pairs(plr.PlayerGui:GetChildren()) do
                if message.Name == "Nurse" then
                    message:Destroy()
                end
		end
            local args = {
                [1] = 1
            }
            
            game:GetService("ReplicatedStorage").DoctorEvent:FireServer(unpack(args))
            repeat wait()
            until plr.PlayerGui:FindFirstChild("GuiMessage")
                                    for i, message in pairs(plr.PlayerGui:GetChildren()) do
                if message.Name == "GuiMessage" then
                	message:Destroy()
            end
		end
            plr.Character.HumanoidRootPart.CFrame = workspace["intern kun man"].HumanoidRootPart.CFrame
            wait(1)
            repeat wait()
            fireclickdetector(workspace["intern kun man"].ClickDetector)
            until plr.PlayerGui:FindFirstChild("Message")
            repeat wait()
            until plr.PlayerGui:FindFirstChild("GuiMessage")
            plr.Character.HumanoidRootPart.CFrame = workspace["other doc man"].HumanoidRootPart.CFrame
            wait(1)
            fireclickdetector(workspace["other doc man"].ClickDetector)
            wait(0.5)
            plr.Character.HumanoidRootPart.CFrame = workspace["head doc man"].HumanoidRootPart.CFrame
            wait(1)
            fireclickdetector(workspace["head doc man"].ClickDetector)
            Rayfield:Notify({
                Title = "Notification",
                Content = "Completeted fast checkup!",
                Duration = 1,
                Image = 4483362458,
                Actions = { -- Notification Buttons
             },
             })
            end,
        })
    end

    if mainid == gameids["level5"] then
        local Gamble = Window:CreateTab("Gambling", 4483362458)

        local Section = Gamble:CreateSection("Main")

        local usetokentoggle = Gamble:CreateToggle({
            Name = "Use tokens",
            CurrentValue = nil,
            Flag = nil,
            Callback = function(callback)
            	local value = "autousetokens"
                    originaldata[value] = callback
                    coroutine.resume(coroutine.create(function()
                    while originaldata[value] do
                        if plr.attributes.Token.Value > 0 then
                            fireclickdetector(workspace.Machine:FindFirstChildOfClass("ClickDetector"))
                        end
                        wait()
                    end
                end))
            end,
         })
         usetokentoggle:Set(originaldata["autousetokens"])

         local Section = Gamble:CreateSection("Extras")

         local collectokentoggle = Gamble:CreateToggle({
            Name = "Auto claim daily tokens",
            CurrentValue = nil,
            Flag = nil,
            Callback = function(callback)
            	local value = "dailytokens"
                    originaldata[value] = callback
                    coroutine.resume(coroutine.create(function()
                        while originaldata[value] == true do
                            if plr.attributes.Token.Value <= 0 then
                                local args = {
                                    [1] = "DAILY_REWARD"
                                }
                                
                                game:GetService("ReplicatedStorage").TimeRewardEvent:FireServer(unpack(args))                        
                            end
                        wait()
                        end
                    end))
            end,
         })
         collectokentoggle:Set(originaldata["dailytokens"])

         local collectokentoggle = Gamble:CreateToggle({
            Name = "Auto convert tokens",
            CurrentValue = nil,
            Flag = nil,
            Callback = function(callback)
            	local value = "converttokens"
                    originaldata[value] = callback
                    coroutine.resume(coroutine.create(function()
                        while originaldata[value] == true do
                            if plr.attributes.Retry.Value >= 5 then
                                game:GetService("ReplicatedStorage").ExchangeEvent:FireServer()                   
                            end
                        wait()
                        end
                    end))
            end,
         })
         collectokentoggle:Set(originaldata["converttokens"])
    end
    


    local Settings = Window:CreateTab("Settings", 4483362458)

    local Section = Settings:CreateSection("Saving")

    local Button = Settings:CreateButton({
        Name = "Save data",
        Callback = function()
            savedata()
            Rayfield:Notify({
                Title = "Notification",
                Content = "Succesfully saved data!",
                Duration = 1,
                Image = 4483362458,
                Actions = { -- Notification Buttons
             },
             })
        end,
    })

    local Label = Settings:CreateLabel("Auto save data saves every 500 seconds")

    local autosavetoggle = Settings:CreateToggle({
        Name = "Auto save data",
        CurrentValue = nil,
        Flag = nil,
        Callback = function(callback)
            local value = "autosave"
                originaldata[value] = callback
                coroutine.resume(coroutine.create(function()
                while originaldata[value] do
                    wait(300)
                    if originaldata[value] then
                        savedata()
                        Rayfield:Notify({
                            Title = "Notification",
                            Content = "Succesfully saved data!",
                            Duration = 1,
                            Image = 4483362458,
                            Actions = { -- Notification Buttons
                               --[[Ignore = {
                                  Name = "Okay!",
                                  Callback = function()
                                  print("The user tapped Okay!")
                               end
                            },]]--
                         },
                         })
                    end
                end
            end))
end,
})
autosavetoggle:Set(originaldata["autosave"])

    local Button = Settings:CreateButton({
        Name = "Delete data",
        Callback = function()
            delfile("TemHubV3/data.txt")
        end,
    })
 
    local Section = Settings:CreateSection("Other")

    local disabseattoggle = Settings:CreateToggle({
        Name = "Disable seats (dont enable while sitting)",
        CurrentValue = nil,
        Flag = nil,
        Callback = function(callback)
            local value = "disableseats"
                originaldata[value] = callback
                for i,seat in pairs(workspace:GetDescendants()) do
                    if seat:IsA("Seat") then
                        seat.Disabled = originaldata[value]
                    end
                 end
        end,
     })
     disabseattoggle:Set(originaldata["disableseats"])

     local laptoptoggle = Settings:CreateToggle({
        Name = "Auto laptop",
        CurrentValue = nil,
        Flag = nil,
        Callback = function(callback)
            local value = "autolaptop"
                originaldata[value] = callback
                coroutine.resume(coroutine.create(function()
                    while originaldata[value] == true do
                        if plr.Backpack:FindFirstChild("Laptop") then
                            plr.Backpack.Laptop.Parent = plr.Character           
                        end
                    wait()
                    end
                end))
        end,
     })
     laptoptoggle:Set(originaldata["autolaptop"])
     
     local onlyonce = false
     local Dropdown = Settings:CreateDropdown({
        Name = "Teleport to floor",
        Options = {"1", "2", "3", "4", "5"},
        CurrentOption = "",
        Flag = nil,
        Callback = function(Option)
         floorteleport(Option)
         if onlyonce == false then
            onlyonce = true
         queueteleport([[
         wait(3)
         loadstring(game:HttpGet("https://raw.githubusercontent.com/kittenis/temhubv3/main/script", true))()
         ]])
         end
        end,
     })

     local Section = Settings:CreateSection("UI")

    local Button = Settings:CreateButton({
        Name = "Destroy UI",
        Callback = function()
            Rayfield:Destroy()
        end,
    })

    end
