    local Button = Settings:CreateButton({
        Name = "Better UI (buttons broken)",
        Callback = function()
            for i, gui in pairs(game.ReplicatedStorage:GetDescendants()) do
                if gui:IsA("Frame") or gui:IsA("ScrollingFrame") then
                if gui.BackgroundTransparency < 1 then
                    Instance.new("UICorner", gui)
                    local stroke = Instance.new("UIStroke", gui)
                    stroke.Thickness = 2
                    stroke.ApplyStrokeMode = "Border"
                    gui.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                    gui.BackgroundTransparency = 0.5
                    end
                elseif gui:IsA("TextLabel") then
                    gui.TextColor3 = Color3.fromRGB(255, 255, 255)
                    Instance.new("UICorner", gui)
                elseif gui:IsA("TextButton") then
                    Instance.new("UICorner", gui)
                    local stroke = Instance.new("UIStroke", gui)
                    stroke.Thickness = 2
                    stroke.ApplyStrokeMode = "Border"
                    gui.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                    gui.BackgroundTransparency = 0.5
                    gui.TextColor3 = Color3.fromRGB(255, 255, 255)
                end
            end

            for i, gui in pairs(plr.PlayerGui:GetDescendants()) do
                if not plr.PlayerGui.Chat:FindFirstAncestor(gui) and not plr.PlayerGui.TopbarPlus:FindFirstAncestor(gui) then
                if gui:IsA("Frame") or gui:IsA("ScrollingFrame") then
                if gui.BackgroundTransparency < 1 then
                    Instance.new("UICorner", gui)
                    local stroke = Instance.new("UIStroke", gui)
                    stroke.Thickness = 2
                    stroke.ApplyStrokeMode = "Border"
                    gui.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                    gui.BackgroundTransparency = 0.5
                    end
                elseif gui:IsA("TextLabel") then
                    gui.TextColor3 = Color3.fromRGB(255, 255, 255)
                    Instance.new("UICorner", gui)
                elseif gui:IsA("TextButton") then
                    Instance.new("UICorner", gui)
                    local stroke = Instance.new("UIStroke", gui)
                    stroke.Thickness = 2
                    stroke.ApplyStrokeMode = "Border"
                    gui.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                    gui.BackgroundTransparency = 0.5
                    gui.TextColor3 = Color3.fromRGB(255, 255, 255)
                end
            end
            end
        end,
    })

