                    if button.Color == Color3.fromRGB(75, 151, 75) then
                        if button:FindFirstChildOfClass("ClickDetector") then
                            fireclickdetector(button:FindFirstChildOfClass("ClickDetector"))
                        end
                    end
                end

                for i, let in pairs(letter) do
                local button = board:FindFirstChild(tostring(let))
                if button:FindFirstChildOfClass("ClickDetector") then
                    fireclickdetector(button:FindFirstChildOfClass("ClickDetector"))
                end
                end
                wait(1)
            end
            for i, button in pairs(board:GetChildren()) do
                if button.Color == Color3.fromRGB(75, 151, 75) then
