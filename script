                                        local stroke = Instance.new("UIStroke", frame.Stuff.Win)
                                        stroke.Thickness = 2
                                        stroke.ApplyStrokeMode = "Border"
                                        frame.Stuff.Win.BackgroundColor3 = Color3.fromRGB(0,0,0)
                                        frame.Stuff.Win.BackgroundTransparency = 0.5
                                        local val = Instance.new("BoolValue", frame)
                                        for i, but in pairs(frame:GetChildren()) do
