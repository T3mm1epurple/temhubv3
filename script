local Button = Drinking:CreateButton({
    Name = "Buy selected drink",
    Callback = function()
        if plr.attributes.Sick.Value > 0 then
            Rayfield:Notify({
                Title = "Sick!",
                Content = "you arent able to buy the drink because youre sick, go to level 4?",
                Duration = 1,
                Image = 4483362458,
                Actions = { -- Notification Buttons
                Ignore = {
                    Name = "No",
                    Callback = function()
                      print("user said no")
                 end
              },
                   Yes = {
                      Name = "Yes",
                      Callback = function()
                        savedata()
                        floorteleport(4)
                        queueteleport([[
                        wait(3)
                        loadstring(game:HttpGet("https://raw.githubusercontent.com/kittenis/temhubv3/main/script", true))()
                        ]])
                        Rayfield:Notify({
                            Title = "Notification",
                            Content = "Teleporting to floor 4..",
                            Duration = 1,
                            Image = 4483362458,
                            Actions = { -- Notification Buttons
                               --[[Ignore = {
                                  Name = "Okay!",
                                  Callback = function()
                                  print("The user tapped Okay!")
                               end
                            },]]--
                         },
                         })
                   end
                },
             },
             })
            return
        end
        if owneringame == false then
        local pos = Vector3.new(0, 0, 0)
        if originaldata["sodamode"] == 3 then
        pos = workspace.RoofP.Position + Vector3.new(0, 10, 0)
        elseif originaldata["sodamode"] == 2 then
        pos = plr.Character.HumanoidRootPart.Position
        end
                    for i,object in pairs(workspace["711Stuff"]:GetChildren()) do
                        if object.Name == originaldata["soda"] then
                            plr.Character.HumanoidRootPart.CFrame = object.Source.CFrame
                            fireproximityprompt(object.Source.Attachment.ProximityPrompt)
                            repeat wait()
                                fireproximityprompt(object.Source.Attachment.ProximityPrompt)
                            until plr.Backpack:FindFirstChild(originaldata["soda"])
                            if originaldata["sodamode"] > 1 then
                            plr.Character.HumanoidRootPart.CFrame = CFrame.new(pos)
                            end
                        end
                    end
                end
    end,
})

