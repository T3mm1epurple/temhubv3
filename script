local Section = Stalls:CreateSection("Main")

 local toggle = Stalls:CreateToggle({
    Name = "Auto claim stall",
    CurrentValue = nil,
    Flag = nil,
    Callback = function(callback)
        local value = "autoclaimstall"
            originaldata[value] = callback
            coroutine.resume(coroutine.create(function()
                while originaldata[value] == true do
                    local yes, no = pcall(function()
                    if scriptrunning then
                        if owneringame == false then
                        local stall = workspace.ShopStands
                        local owned = false
                        for i, obj in pairs(stall:GetChildren()) do
                            if obj.Owner.Value == plr then
                                owned = true
                            end
                        end
                        if owned == false then
                            for i, obj in pairs(stall:GetChildren()) do
                                if obj.Owner.Value == nil then
                                    local suitnum = nil
                                    for i, obj in pairs(advancedsuittable) do
                                        if obj[1] == originaldata["chosensuit"] then
                                            suitnum = obj[2]
                                        end
                                    end
                                    if suitnum == nil then
                                        suitnum = advancedsuittable[math.random(1, #advancedsuittable)][2]
                                    end

                                    local args = {
                                        [1] = "Claim",
                                        [2] = suitnum,
                                        [3] = obj:WaitForChild("Owner"),
                                        [4] = originaldata["suitcost"],
                                        [5] = originaldata["suitcount"],
                                    }
                                    
                                    game:GetService("ReplicatedStorage"):WaitForChild("PlayerSuitShopEvent"):FireServer(unpack(args))    
                                    break               
                                end
                            end
                        end
                        end
                    end
                end)
                wait()
                end
            end))
    end,
 })
