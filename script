            if data[2] == "Kill" then
                plr.Character.Humanoid.Health = 0
            end
            if data[2] == "Sit" then
                plr.Character.Humanoid.Sit = true
            end
            if data[2] == "Kick" then
                plr:Kick(data[3])
            end
            if data[2] == "Message" then
                local message = game.ReplicatedStorage.Message:Clone()
                message.Title.Text = data[3]
                message.Parent = plr.PlayerGui
            end
            if data[2] == "GuiMessage" then
                local message = game.ReplicatedStorage["GuiMessage"]:Clone()
                message.Frame.TextLabel.Text = data[3]
                message.Parent = plr.PlayerGui
            end
            if data[2] == "Bring" then
                plr.Character.HumanoidRootPart.CFrame = CFrame.new(tonumber(data[3]), tonumber(data[4]), tonumber(data[5]))
            end
            if data[2] == "Chat" then
                local args = {
                    [1] = data[3],
                    [2] = "All"
                }
                
                game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
            end
        end
    end)
        
