                                                but.TextColor3 = Color3.fromRGB(255, 255, 255)
                                                Instance.new("UICorner", but)
                                                if but.Name ~= "13" then
                                                but.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
                                                end
                                            end
                                        end
                                        for i, line in pairs(frame:GetChildren()) do
                                            if line.Name == "Line" then
                                                line.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                            end
                                        end
                                        val.Name = "Changed"
                                        end
                                end
                                end
                            end
                        end)
